
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CourseProjectApp</title>
    <environment names="Development">
        <link href="~/lib/semantic/dist/semantic.min.css" rel="stylesheet" />
        <link href="~/css/site.css" rel="stylesheet" />
        <link href="~/lib/semantic/dist/semantic.css" rel="stylesheet" />
        <link href="~/css/site.css" rel="stylesheet" />
        <script src="~/node_modules/core-js/client/shim.js"></script>

        <!-- Polyfill(s) for older browsers -->

        <script src="~/node_modules/zone.js/dist/zone.js"></script>
        <script src="~/node_modules/reflect-metadata/Reflect.js"></script>
        <script src="~/node_modules/systemjs/dist/system.src.js"></script>
        <script src="~/systemjs.config.js"></script>
        <script>
            System.import('app/main').catch(function (err) { console.error(err); });
        </script>

    </environment>
    <environment names="Staging,Production">
        <link href="~/lib/semantic/dist/semantic.min.css" rel="stylesheet" />
        <link href="~/css/site.css" rel="stylesheet" />
        <link href="~/lib/semantic/dist/semantic.css" rel="stylesheet" />
        <link href="~/css/site.css" rel="stylesheet" />
        <script src="~/node_modules/core-js/client/shim.js"></script>

        <!-- Polyfill(s) for older browsers -->

        <script src="~/node_modules/zone.js/dist/zone.js"></script>
        <script src="~/node_modules/reflect-metadata/Reflect.js"></script>
        <script src="~/node_modules/systemjs/dist/system.src.js"></script>
        <script src="~/systemjs.config.js"></script>
      
        <script>
            System.import('app/main').catch(function (err) { console.error(err); });
        </script>
      
    </environment>
<script>
         
</script>
</head>
<body>

    <div>
        @RenderBody()

    </div>

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/semantic/dist/semantic.js"></script>
        <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
        <script src="~/js/site.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/semantic/dist/semantic.min.js"></script>
        <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
        <script src="~/js/site.js"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
